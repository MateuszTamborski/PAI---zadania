@{ 
    var rules = (List<string>)Session["rules"];
    if (rules == null)
    {
        rules = new List<string>();
    }
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <form action="~/RulesRedirect.cshtml" method="post">
        <input type="text" name="rule" placeholder="Rule" />
        <input type="submit" value="Submit" />
    </form>
    <ol>
        @foreach(var rule in rules)
        {
            <li>@rule</li>
        }
    </ol>
</body>
</html>
